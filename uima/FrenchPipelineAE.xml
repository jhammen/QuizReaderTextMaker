<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="WiktAnnotatorAE">
      <import location="WiktAnnotatorAE.xml"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="QRXCasConsumer">
      <import location="QRXCasConsumer.xml"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchUnicodeTokenizer-AE">
      <import name="tddts.preprocessing.ae.FrenchUnicodeTokenizer-AE"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="OutputFileAE">
      <import location="OutputFileAE.xml"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="MarkupAnnotator">
      <import location="MarkupAnnotator.xml"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>aeDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback"/>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>MarkupAnnotator</node>
        <node>FrenchUnicodeTokenizer-AE</node>
        <node>WiktAnnotatorAE</node>
        <node>OutputFileAE</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">org.quizreader.textmaker.uima.types.WiktAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.tika.MarkupAnnotation</type>
          <type allAnnotatorFeatures="true">org.quizreader.textmaker.uima.types.OutputFileAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TokenAnnotation</type>
        </outputs>
        <inputSofas>
          <sofaName>pipelineTextView</sofaName>
        </inputSofas>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
  <sofaMappings>
    <sofaMapping>
      <componentKey>MarkupAnnotator</componentKey>
      <componentSofaName>textView</componentSofaName>
      <aggregateSofaName>pipelineTextView</aggregateSofaName>
    </sofaMapping>
    <sofaMapping>
      <componentKey>FrenchUnicodeTokenizer-AE</componentKey>
      <aggregateSofaName>pipelineTextView</aggregateSofaName>
    </sofaMapping>
    <sofaMapping>
      <componentKey>WiktAnnotatorAE</componentKey>
      <aggregateSofaName>pipelineTextView</aggregateSofaName>
    </sofaMapping>
    <sofaMapping>
      <componentKey>OutputFileAE</componentKey>
      <aggregateSofaName>pipelineTextView</aggregateSofaName>
    </sofaMapping>
  </sofaMappings>
</analysisEngineDescription>
